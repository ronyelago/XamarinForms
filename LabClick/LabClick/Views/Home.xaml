<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LabClick.Views.Home"
             BackgroundImage="bghome.jpg">
    <ContentPage.Content>
        <StackLayout Margin="10,10,10,10"
                     VerticalOptions="CenterAndExpand">
            <Button Image="home1.png"
                    BackgroundColor="Transparent"
                    x:Name="NovoTeste"
                    Clicked="NovoTeste_Clicked"/>
            <Button Image="home2.png"
                    BackgroundColor="Transparent"
                    x:Name="NovoPaciente"
                    Clicked="NovoPaciente_Clicked"/>
            <Button Image="home3.png"
                    BackgroundColor="Transparent"/>
        </StackLayout>
        
    </ContentPage.Content>
</ContentPage>